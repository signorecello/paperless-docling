# Paperless-docling environment configuration

# API Connection Settings
PAPERLESS_API_URL=https://your-paperless-instance.com/api
PAPERLESS_AUTH=Basic your-base64-encoded-credentials

# Tag Processing Settings
TAG_NAME=docling
CHECK_INTERVAL=60000

# Docling Settings
DOCLING_PIPELINE=vlm
DOCLING_MODEL=smoldocling 
DOCLING_DEVICE=cuda
DOCLING_THREADS=32
DOCLING_PDF_BACKEND=dlparse_v4
DOCLING_EXTRA_ARGS=

# Server Settings
PORT=3000 
